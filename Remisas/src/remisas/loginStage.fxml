<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="500.0" prefWidth="800.0" stylesheets="@estilos.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="remisas.LoginController">
   <children>
      <VBox id="menuPanel" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label id="title" prefHeight="71.0" prefWidth="409.0" text="Inicia Sesion" />
            <Label text="Usuario:" />
            <TextField fx:id="textUsuario" />
            <Label text="Contraseña:" />
            <TextField fx:id="textContra" />
            <Button fx:id="buttonLogin" mnemonicParsing="false" text="Iniciar Sesion" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Button fx:id="buttonLogout" mnemonicParsing="false" text="Registrarse" />
                  <Label fx:id="labelForgotPassword" text="¿Olvidaste tu contraseña?" />
               </children>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets left="30.0" right="30.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
